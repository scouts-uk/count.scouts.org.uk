---
# Site specific configurations...
  logins_enabled:   false
  use_google_fonts: true
  development:      false
# Database configuration
  db:
    name: census2022
    user: db_user
    pass: db_pass
# Compass authentication configuration.
  auth:
    url: auth_url
    # Headers for authentication
    head:
      - auth_head...
      - ...
    # Template for body of authentication
    template:
      - auth_tmpl...
      - auth_tmpl...
      - ...
# User cookie name/crypt/secret etc
  crypt:
    cookie: cookie_name
    method: crypt_method
    key:    crypt_key
    secret: cookie_secret

